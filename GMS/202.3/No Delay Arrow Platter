define(delay, #%arg1) //Adjust Delay to Spawn Rate

[Enable]
Alloc(Hook,123)
Label(Return)
Label(Exit)

alloc(timer,4)

Hook:
pushad
// 74 ? E8 ? ? ? ? 50 FF ? ? E8 ? ? ? ? 83 C4 08 84 C0 74
call 02772770 //get_update_time (using this instead of actual function delay because its timer is sort of werid)
mov edx,eax
sub edx,[timer]
cmp edx,delay
popad
jl Exit
mov [timer],eax
jmp 01324AE2+6 //disable address +6

Exit:
jmp 01324AFB //Jump to exit so you don't jump back ontop of more delay.

01324AE2:
jmp Hook
nop
Return:

[Disable]
Dealloc(Hook)

01324AE2: // [above] 76 13 6A 00 8D
sub ecx,edx
cmp ecx,eax
jna 01324AFB